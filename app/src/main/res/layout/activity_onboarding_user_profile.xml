<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:bind="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="viewModel"
            type="io.tipblockchain.kasakasa.ui.onboarding.profile.OnboardingUserProfileViewModel"/>
    </data>
    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="io.tipblockchain.kasakasa.ui.onboarding.profile.OnboardingUserProfileActivity">

        <include layout="@layout/content_onboarding_user_profile"
            bind:viewModel="@{viewModel}"/>

    </android.support.design.widget.CoordinatorLayout>
</layout>

